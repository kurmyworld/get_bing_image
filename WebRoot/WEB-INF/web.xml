<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <servlet-name>FromURL</servlet-name>
    <servlet-class>cn.chioy.work.FromURL</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FromCache</servlet-name>
    <servlet-class>cn.chioy.work.FromCache</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GetURL</servlet-name>
    <servlet-class>cn.chioy.work.GetURL</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FromURL</servlet-name>
    <url-pattern>/url</url-pattern>
    <url-pattern>/from-URL</url-pattern>
    <url-pattern>/from-url</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FromCache</servlet-name>
    <url-pattern>/cache</url-pattern>
    <url-pattern>/from-cache</url-pattern>
    <url-pattern>/from-Cache</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetURL</servlet-name>
    <url-pattern>/GetURL</url-pattern>
    <url-pattern>/geturl</url-pattern>
    <url-pattern>/get-URL</url-pattern>
    <url-pattern>/get-url</url-pattern>
  </servlet-mapping>
</web-app>